                        jQuery(document).ready( function () {
                            jQuery("#send_to_menu input[type='radio']").click( function () {
                                var selectedValue = jQuery(this).val().toLowerCase();
                                var selectedDiv = jQuery("#send_to_menu div." + selectedValue);
                                
